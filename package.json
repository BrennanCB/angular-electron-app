{
  "name": "angular-electron",
  "version": "0.1.0",
  "description": "Test app for angular electron",
  "keywords": [
    "angular",
    "electron",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "ng": "nx",
    "electron": "electron",
    "start": "npm run app:start:dev",
    "config": "ts-node ./libs/whitelabels/src/lib/scripts/setenv.ts",
    "clean": "rm -rf apps/electron-app/electron/node_modules && rm -rf apps/electron-app/dist",
    "app:start:dev": "npm-run-all app:remove-dist -p app:electron:wait-for-build:dev app:watch:dev",
    "app:start:prod": "npm-run-all app:remove-dist -p app:electron:wait-for-build app:watch:prod",
    "app:remove-dist": "rm -rf dist/apps/electron-app",
    "app:electron:build-main": "webpack build --config apps/electron-app/electron/webpack.config.js",
    "app:electron:serve": "npm run app:electron:build-main && electron ./apps/electron-app/electron/main.js --serve",
    "app:electron:serve:dev": "npm run app:electron:build-main && electron ./apps/electron-app/electron/main.js --serve --no-csp",
    "app:electron:run": "electron ./apps/electron-app/electron/main.js",
    "app:electron:wait-for-build": "node apps/electron-app/scripts/wait-for-build.js && npm run app:electron:serve",
    "app:electron:wait-for-build:dev": "node apps/electron-app/scripts/wait-for-build.js && npm run app:electron:serve:dev",
    "app:build:dev": "nx run electron-app:build:dev --base-href ./",
    "app:build:prod": "nx run electron-app:build:production --base-href ./",
    "app:watch:dev": "nx run electron-app:build:dev --base-href ./ --watch=true",
    "app:watch:prod": "nx run electron-app:build:production --base-href ./ --watch=true",
    "app:local:dev": "npm run app:build:dev && npm run app:electron:serve:dev",
    "app:local:prod": "npm run app:build:prod && npm run app:electron:serve",
    "app:package:prod": "npm run app:build:prod && npm run app:electron:build-main",
    "app:package:dev": "npm run app:build:dev && npm run app:electron:build-main",
    "app:package": "npm run config && npm run app:electron:build-main && nx run electron-app:build --base-href ./",
    "app:electron:build": "electron-builder build --config=apps/electron-app/electron-builder.json --publish=never -mwl",
    "app:electron:build-local": "electron-builder build --config=apps/electron-app/electron-builder.json --publish=never",
    "app:electron:publish": "electron-builder build --config=apps/electron-app/electron-builder.json --publish=always -mwl",
    "test": "nx test --watch=false",
    "test:watch": "nx test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "lint": "nx lint",
    "app:version:patch": "nx run electron-app:version --releaseAs=patch",
    "app:version:minor": "npx run electron-app:version --releaseAs=minor",
    "app:version:major": "npx run electron-app:version --releaseAs=major",
    "app:version:prerelease": "npx lerna version prerelease",
    "build-affected": "nx --target=build",
    "build-ts-api": "nx run ts-api:build",
    "build:libs": "nx run-many --target=build",
    "version-affected:libs": "nx run-many --target=version-lib --parallel=1",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/cdk": "~14.0.0",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "~14.0.0",
    "@angular/forms": "~14.0.0",
    "@angular/localize": "~14.0.0",
    "@angular/material": "~14.0.0",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@commitlint/config-angular": "17.1.0",
    "@ngxs/devtools-plugin": "^3.7.5",
    "@ngxs/store": "^3.7.5",
    "@nrwl/angular": "14.5.2",
    "@nrwl/node": "14.5.2",
    "dotenv": "16.0.2",
    "electron-log": "4.4.8",
    "electron-store": "8.1.0",
    "fs-extra": "10.1.0",
    "keytar": "7.9.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.3.0",
    "uuid": "9.0.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~14.0.0",
    "@angular-devkit/build-angular": "~14.0.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "~14.0.0",
    "@angular/language-service": "~14.0.0",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@jscutlery/semver": "~2.27.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@nrwl/cli": "~14.7.13",
    "@nrwl/cypress": "~14.7.11",
    "@nrwl/eslint-plugin-nx": "~14.7.11",
    "@nrwl/jest": "~14.7.11",
    "@nrwl/linter": "~14.7.11",
    "@nrwl/workspace": "~14.7.11",
    "@playwright/test": "1.23.4",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "conventional-changelog-cli": "2.2.2",
    "cypress": "10.8.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-webpack": "8.0.1",
    "electron": "20.2.0",
    "electron-builder": "23.3.3",
    "electron-debug": "3.2.0",
    "electron-notarize": "1.2.1",
    "electron-reload": "2.0.0-alpha.1",
    "electron-reloader": "1.2.3",
    "electron-updater": "5.2.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "8.0.1",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "27.5.1",
    "jest-auto-spies": "1.6.10",
    "jest-environment-jsdom": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "json": "11.0.0",
    "karma": "6.4.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lerna": "5.5.2",
    "lerna-changelog": "2.2.0",
    "ngx-deploy-npm": "4.2.4",
    "node-polyfill-webpack-plugin": "2.0.0",
    "npm-run-all": "4.1.5",
    "nx": "~14.7.11",
    "path-browserify": "1.0.1",
    "playwright": "1.23.4",
    "prettier": "2.7.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "27.1.4",
    "ts-loader": "9.4.1",
    "ts-node": "10.8.2",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.7.4",
    "vite": "3.1.5",
    "wait-on": "6.0.1",
    "webdriver-manager": "12.1.8",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome 100"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
